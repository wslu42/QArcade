pico-8 cartridge // http://www.pico-8.com
version 32
__lua__
function _init()
 x=30
 y=30
 xmin,xmax,ymin,ymax=0,40,0,48
 oxl,oxr,oyu,oyd=24,40,12,26
 collide = false
 
end

function _update()

 if btn(➡️) then
  if(x<xmax) x+=1
 end
 if btn(⬅️) then
  if(x>xmin) x-=1
 end
 if btn(⬆️) then
  if(y>ymin) y-=1
 end
 if btn(⬇️) then
  if(y<ymax) y+=1
 end
 
 if (x>oxl and x<oxr and y>oyu and y<oyd) then
  sfx(0)
  collide = true
 end
 
end

function _draw()
 cls()
 map()
 
 if not collide then
  spr(17,4*8,2*8)
 end

 if (btn(❎)) collide = false
 
 spr(1,x,y)
end

