function bint2dect(b,sort)
 local d = {}

 for i,v in pairs(b) do
  local n = {}
  for j,u in pairs(v) do
   n[j] = bin2dec(u)
  end

  if #n==1 then
   d[i] = tostr(n[1])
  elseif #n==2 and sort then
   d[i] = min(n[1],n[2])..max(n[1],n[2])
  else
   d[i] = n[1]..n[2]
  end
 
 end
 return(d)
end
