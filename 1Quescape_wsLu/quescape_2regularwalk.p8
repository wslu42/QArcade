function fltn(tb,rank)
-- return flattened table from
--rank 2 to rank 4
 local tb_fl = {}

 if rank==2 then
  for i,v in pairs(tb) do
   for j,u in pairs(v) do
    add(tb_fl,u) end end    

 elseif rank==3 then
  for i,v in pairs(tb) do
   for j,u in pairs(v) do
    for k,t in pairs(u) do
    add(tb_fl,t) end end end    
 
 elseif rank==4 then
  for i,v in pairs(tb) do
   for j,u in pairs(v) do
    for k,t in pairs(u) do
     for k,s in pairs(t) do
     add(tb_fl,s) end end end end    

 end
 return(tb_fl)
end
