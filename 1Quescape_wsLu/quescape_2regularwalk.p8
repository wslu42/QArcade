function as_bits (num,bits,idn)
-- convert decimal to bintable
-- set idn="s" to binstring
 local bitstring = {}
 for index = bits, 1, -1 do
     b = num - flr(num/2)*2
     num = flr((num - b) / 2)
     bitstring[index] = b
 end

 if idn=="s" then
  bitstring = tb2str(bitstring)
 end
 return bitstring
end
