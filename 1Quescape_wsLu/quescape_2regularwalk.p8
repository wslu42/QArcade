function dect2bint(d)
 local b = {}
 for i,v in pairs(d) do
  local a1 = {}
  for j=1,#v do
   a0 = as_bits(sub(v,j,j),3,"_")
   a1[j] = a0
  end
  b[i] = a1
 end
 return b
end
