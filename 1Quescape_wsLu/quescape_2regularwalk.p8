function _qcmeas(qc,llm)
 for i = 0,2,1 do
  qc.measure(i,i) end
 local n = prtct(simulate(qc,"counts",1))
 local nst,nct = n[1],n[2]
 local possiblestt = {}
 for i,v in pairs(nct) do
  if tonum(v)>llm then
   possiblestt[i]=nst[i] end
 end
 return(possiblestt)
end
